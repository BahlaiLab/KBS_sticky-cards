old21 <- insects_all[which(insects_all$CARDYEAR=="Old21"),]
#combine
library (plyr)
old20_old21 <- rbind.fill (old20, old21)
insects.rowsums <- rowSums(old20_old21[,6:24]>0)
old20_old21$richness <- insects.rowsums
